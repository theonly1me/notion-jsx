{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "react",
    "incremental": true,
    "plugins": [],
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "notion-to-jsx": [
        "./dist/types/index.d.ts"
      ]
    },
    "declaration": true,
    "declarationDir": "./dist/types",
    "outDir": "./dist",
    "types": [
      "node",
      "react"
    ]
  },
  "include": [
    "**/*.ts",
    "**/*.tsx"
  ],
  "exclude": [
    "node_modules",
    "./dist/**/*"
  ],
  "moduleResolution": "node",
  "allowSyntheticDefaultImports": true,
  "esModuleInterop": true
}